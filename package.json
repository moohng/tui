{
  "name": "tui",
  "version": "1.0.0",
  "description": "tiny ui",
  "main": "dist/tui.umd.js",
  "module": "lib/index.js",
  "types": "packages/index.d.ts",
  "scripts": {
    "serve": "parcel serve index.html --no-cache",
    "build:css": "gulp css",
    "build:js": "babel packages -d lib -x \".ts,.tsx\"",
    "build:types": "tsc --emitDeclarationOnly",
    "build:lib": "npm run build:types && npm run build:js && npm run build:css",
    "build:dist": "rollup -c",
    "type-check": "tsc --noEmit",
    "test": "jest"
  },
  "keywords": [
    "ui",
    "tui"
  ],
  "author": "Kevin",
  "license": "MIT",
  "dependencies": {
    "@moohng/dan": "^2.0.2"
  },
  "devDependencies": {
    "@babel/cli": "^7.12.10",
    "@babel/core": "^7.12.10",
    "@babel/plugin-proposal-class-properties": "^7.12.13",
    "@babel/plugin-proposal-nullish-coalescing-operator": "^7.12.1",
    "@babel/plugin-proposal-optional-chaining": "^7.12.7",
    "@babel/preset-env": "^7.12.11",
    "@babel/preset-react": "^7.12.10",
    "@babel/preset-typescript": "^7.12.7",
    "@rollup/plugin-babel": "^5.2.3",
    "@rollup/plugin-node-resolve": "^11.1.1",
    "@types/jest": "^26.0.20",
    "autoprefixer": "^10.2.3",
    "babel-jest": "^26.6.3",
    "del": "^6.0.0",
    "esm": "^3.2.25",
    "gulp": "^4.0.2",
    "gulp-babel": "^8.0.0",
    "gulp-postcss": "^9.0.0",
    "gulp-sass": "^4.1.0",
    "jest": "^26.6.3",
    "parcel": "^2.0.0-beta.1",
    "postcss": "^8.2.4",
    "rollup": "^2.38.4",
    "rollup-plugin-terser": "^7.0.2",
    "sass": "^1.32.5",
    "typescript": "^4.1.3"
  },
  "browserslist": [
    "last 1 version",
    "> 0.5% in cn",
    "not ie 8",
    "not dead"
  ]
}
